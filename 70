#include <stdio.h>

int calcSum(int arr[], int len){
    int sum = 0;
    
    for(int i = 0; i < len; i++)
        sum = sum + arr[i];
        
    return sum;
}
int main()
{
    int arr[] = {1, 5, 2, 10, 4, 8};
    
    // get the length of the array
    int len = sizeof(arr)/sizeof(arr[0]);    
    
    printf("Sum: %d",calcSum(arr, len));
}
