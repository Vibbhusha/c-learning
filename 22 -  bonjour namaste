#include<stdio.h>

void namaste();
void bonjour();

int main(){
    printf("enter f for french & i for indian");
    char ch;
    scanf("%c",&ch);
    if(ch=='f'){
       printf("bonjour");
    }else{
        printf("namaste");
    }
    
    return 0;
}

void namaste() {
    printf("namaste\n");
}
void bonjour() {
    printf("bonjour\n");
}
