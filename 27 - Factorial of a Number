#include<stdio.h>
int main () {
    int num , fact = 1;
    printf("enter a number");
    scanf("%d",&num);
    
    if(num<0) {
        printf("error");
    }else{
        for(int i=1; i<=num; i++)
        fact=fact*i;
        printf("the factorial of %d is %d" , num , fact);
    }
      return 0;
}
